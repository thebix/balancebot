{
  "name": "yenombot",
  "version": "0.5.0",
  "description": "group stats",
  "keywords": [
    "telegram",
    "bot",
    "balance"
  ],
  "main": "dist/server.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "start": "nodemon --trace-deprecation src/server.js --exec babel-node",
    "debug": "node src/server.js",
    "build": "export NODE_ENV=production&& babel src -d dist",
    "serve": "export NODE_ENV=production&& node dist/server.js",
    "buildW": "set NODE_ENV=production&& babel src -d dist",
    "serveW": "set NODE_ENV=production&& node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebix/yenombot.git"
  },
  "author": "Paul Byron",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/thebix/yenombot/issues"
  },
  "homepage": "https://github.com/thebix/yenombot#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "forever": "~0.15.3"
  },
  "dependencies": {
    "expr-eval": "^1.0.0",
    "json2csv": "^3.7.3",
    "jsonfile": "^2.4.0",
    "node-telegram-bot-api": "^0.27.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "private": true,
  "engines": {
    "node": ">= 0.6.0",
    "npm": ">= 1.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": true,
    "retainLines": true
  }
}
