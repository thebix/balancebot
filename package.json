{
    "name": "balancebot",
    "version": "0.0.1",
    "description": "group stats",
    "keywords": [
        "telegram",
        "bot",
        "balance"
    ],
    "main": "server.js",
    "scripts": {
        "test": "mocha --compilers js:babel-register",
        "start": "nodemon src/server.js --exec babel-node",
        "debug": "nodemon --debug src/server.js --exec babel-node",
        "build": "export NODE_ENV=production&& set NODE_ENV=production&& babel src -d dist",
        "serve": "export NODE_ENV=production&& set NODE_ENV=production&& node dist/server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JonathanZWhite/bot-server.git"
    },
    "author": "Paul Byron",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/thebix/balancebot/issues"
    },
    "homepage": "https://github.com/thebix/balancebot#readme",
    "devDependencies": {
        "babel-cli": "^6.18.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-stage-2": "^6.18.0",
        "babel-register": "^6.18.0",
        "mocha": "^3.2.0",
        "nodemon": "^1.11.0",
        "forever": "~0.15.3"
    },
    "dependencies": {
        "redux": "^3.6.0",
        "node-telegram-bot-api": "^0.27.0",
        "jsonfile": "^2.4.0"
    }
}